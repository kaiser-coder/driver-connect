<div class="row">
	<div class="col-12 py-5">
		<h4>Zones de groupes</h4>
	</div>
</div>
<div class="row">
	<div class="col-lg-12">
		<div class="grid">
			<p class="grid-header">Liste des zones de groupes</p>
			<div class="grid-body">
				<div class="item-wrapper">
					<div class="row mb-3">
						<div class="col-md-12 mx-auto" ng-app="myapp">
							<table class="table table-striped ng-data-table" style="width:100%">
								<thead>
									<tr>
										<th>Product</th>
										<th>Qty</th>
										<th>Amount</th>
										<th>Profit</th>
										<th></th>
									</tr>
								</thead>
								<tbody ng-init="comments=[{id: 1, nom: 'cola'}, {id: 2, nom: 'coca'}]">
									<tr ng-repeat="comment in comments">
										<td>{{ comment.nom }}</td>
										<td>874</td>
										<td>$546</td>
										<td>43%</td>
										<td class="actions">
											<i class="mdi mdi-dots-vertical"></i>
										</td>
									</tr>
								</tbody>
							</table>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>
